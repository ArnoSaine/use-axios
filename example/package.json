{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@arnosaine/react-scripts": "2.1.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-error-boundary": "^1.2.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint {.,src/**}/*.{js,mjs} --fix",
    "lint": "npm run eslint && npm run prettier",
    "prepack": "npm run build",
    "prettier": "npm run prettier:write -- {.,src/**}/*.{css,graphql,html,js,json,less,md,mdx,mjs,scss,ts,yaml} !./package-lock.json",
    "prettier:write": "prettier --config .prettierrc.js --write",
    "preversion": "npm run lint"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{js,mjs}": [
      "eslint --fix",
      "npm run prettier:write",
      "git add"
    ],
    "*.{css,graphql,html,json,less,md,mdx,scss,ts,yaml}": [
      "npm run prettier:write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "dotenv-webpack": "^1.7.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5"
  }
}
